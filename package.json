  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:*:fix\"",
    "lint:hbs": "ember-template-lint \"{app,tests}/**/*.hbs\"",
    "lint:hbs:fix": "ember-template-lint \"{app,tests}/**/*.hbs --fix\"",
    "lint:js": "eslint \"{app,tests}/**/*.{ts,js}\" --cache",
    "lint:js:fix": "eslint \"{app,tests}/**/*.{ts,js}\" --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:duplication": "jscpd --exitCode 1 --pattern \"app/**/*.{ts,js}\"",
    "test:ember": "COVERAGE=true ember test",
    "test:cov": "node read-cov.js . 40",
    "postinstall": "husky install"
  },
